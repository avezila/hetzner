{
  "editor.tabSize": 2,
  "javascript.validate.enable": false,
  "extensions.autoUpdate": true,
  "docker.enableLinting": true,
  "workbench.iconTheme": null,
  "html.format.enable": false,
  "html.validate.styles": false,
  "html.validate.scripts": false,
  "css.validate": false,
  "scss.validate": false,
  "tslint.run": "onSave",
  "window.zoomLevel": 0,
  "workbench.sideBar.location": "left",
  "search.useIgnoreFilesByDefault": true,
  "editor.minimap.enabled": false,
  "editor.fontSize": 14,
  "typescript.check.tscVersion": false,
  "python.pythonPath": "python3.5",
  "python.formatting.provider": "autopep8",
  "python.formatting.autopep8Path": "python3.5 autopep8",
  "go.useCodeSnippetsOnFunctionSuggest": false,
  "go.autocompleteUnimportedPackages": true,
  "go.useLanguageServer": true,
  "go.gotoSymbol.includeImports": true,
  "go.lintTool": "gometalinter",
  "go.docsTool": "guru",
  "go.buildOnSave": "package",

  "shellcheck.enable": true,
  "shellcheck.run": "onType",
  "shellcheck.executablePath": "shellcheck",
  "shellcheck.exclude": [],
  "shellcheck.customArgs": [],
  "shellcheck.ignorePatterns": {},
  "shellcheck.useWSL": false,
  "shellformat.flag": "-i 2",

  "[go]": {
    "editor.tabSize": 4,
    "editor.insertSpaces": false
  },
  "go.vetFlags": ["-composites"],
  "editor.formatOnSave": true,
  "editor.formatOnPaste": false,
  // Note, these settings should match lint-staged settings!
  "prettier.printWidth": 100,
  "prettier.tabWidth": 2,
  "prettier.useTabs": false,
  "prettier.semi": true,
  "prettier.singleQuote": false,
  "prettier.trailingComma": "es5",
  "prettier.bracketSpacing": false,
  "prettier.jsxBracketSameLine": false,
  "prettier.arrowParens": "avoid",
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.wordWrapColumn": 100,
  "editor.rulers": [100],
  "files.exclude": {
    "**/.editorconfig": true,
    "**/.prettierrc": true,
    "**/.vscode": true,
    "**/LICENSE": true,
    "**/node_modules": true,
    "**/yarn.lock": true
  },
  "bashBeautify.tabSize": 2
  // "javascript.validate.enable": false // enable for flow
}
